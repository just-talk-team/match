pipeline {
    agent any

    tools {
        nodejs '12.18.4'
    }

    options {
        timeout(time: 2, unit: 'MINUTES')
    }

    stages {
        stage('Install from yarn a') {
            steps {
                sh 'yarn install'
            }
        }
        stage('TestsA') {
            steps {
                sh 'yarn test'
            }
        }
    }
}